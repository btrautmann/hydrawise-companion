{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "irri",
            "request": "launch",
            "type": "dart",
            "program": "app/lib/main_production.dart",
            "flutterMode": "debug",
            "args": [
                "--dart-define",
                "OPEN_WEATHER_MAP_API_KEY=${env:OPEN_WEATHER_MAP_API_KEY}",
                "--flavor",
                "production"
            ]
        },
        {
            "name": "irri (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "app/lib/main_production.dart",
            "args": [
                "--dart-define",
                "OPEN_WEATHER_MAP_API_KEY=${env:OPEN_WEATHER_MAP_API_KEY}",
                "--flavor",
                "production"
            ]
        },
        {
            "name": "app tests",
            "request": "launch",
            "type": "dart",
            "program": "app/test/",
            "args": [
                "--test-randomize-ordering-seed=random"
            ]
        }
    ]
}